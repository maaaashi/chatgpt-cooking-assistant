generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Recipe {
  id       Int    @id @default(autoincrement())
  recipe   String
  prompt   String
  imageUrl String
}
